{
  "Logging": {
    "IncludeScopes": false,
    "PathFormat": "wwwroot/App_Data/Logs/IOA_Web_{Date}.txt",
    "RetainedFileCountLimit": "30",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "Yami": {
    "Settings": {
      "Debug": "false",
      "Scheme": "https",
      "Host": "api.irmaonair.com",
      "Port": "443",
      "Path": "services/REST/apc",
      "Version": "v1",
      "Release": "r6",
      "ZipCompressionLevel": "Fastest",
      "RESTCompressionLevel": "GZip",
      "TimeoutREST": "5",
      "DayLimit": "31",
      "SessionTimeout": "30240",
      "SessionCache": "false"
    },
    "SettingsEx": {
      "User": "user/devices",
      "Stops": "stops",
      "Reports": "reports",
      "Tracks": "tracks"
    }
  }
}
/* Note: For appsettings help, please look into the documentation. */